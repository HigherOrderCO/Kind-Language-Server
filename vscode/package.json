{
  "name": "eszopiclone",
  "displayName": "Eszopiclone",
  "description": "Kind Language Server support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "kind2",
        "aliases": [
          "Kind2",
          "kind2"
        ],
        "extensions": [
          "kind2",
          "kind"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/kind-light.svg",
          "dark": "./icons/kind-dark.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "kind2",
        "scopeName": "Main.kind2",
        "path": "./syntaxes/kind2.tmLanguage.json"
      }
    ]
  }
}
