{
  "name": "eszopiclone",
  "displayName": "Eszopiclone",
  "description": "Kind programming language support",
  "version": "0.3.1",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aripiprazole/eszopiclone"
  },
  "publisher": "kind-language-server",
  "contributes": {
    "grammars": [
      {
        "language": "kind",
        "scopeName": "source.kind",
        "path": "./syntaxes/kind.tmLanguage.json"
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "punctuation",
        "description": "Punctuation symbols"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "punctuation": [
            "punctuation"
          ],
          "type.modification": [
            "storage.modifier"
          ]
        }
      }
    ],
    "languages": [
      {
        "id": "kind",
        "aliases": [
          "Kind",
          "kind2"
        ],
        "extensions": [
          "kind",
          "kind2"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/kind-light.svg",
          "dark": "./icons/kind-dark.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vscode:package": "node ./node_modules/.bin/vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "activationEvents": [
    "onLanguage:kind"
  ],
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "@vscode/vsce": "^2.19.0",
    "prettier": "^2.8.7",
    "tree-sitter": "^0.20.1",
    "tree-sitter-kind": "^0.0.1",
    "typescript": "^5.0.4",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  }
}
