{
  "name": "eszopiclone",
  "displayName": "Eszopiclone",
  "description": "Kind programming language support",
  "version": "0.3.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aripiprazole/eszopiclone"
  },
  "publisher": "kind-language-server",
  "contributes": {
    "languages": [
      {
        "id": "kind",
        "aliases": [
          "Kind",
          "kind2"
        ],
        "extensions": [
          "kind",
          "kind2"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/kind-light.svg",
          "dark": "./icons/kind-dark.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "kind",
        "scopeName": "source.kind",
        "path": "./syntaxes/kind.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "tree-sitter": "^0.20.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  }
}
